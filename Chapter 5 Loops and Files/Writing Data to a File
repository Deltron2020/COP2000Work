// this program writes data to a file
#include <iostream>
#include <fstream>
using namespace std;

int main()
{

    ofstream outputFile;
    outputFile.open("demofile.txt");

    cout << "Now writing data to the file.\n";

    // write four names to the file
    outputFile << "You\n";
    outputFile << "Can\n";
    outputFile << "Suck\n";
    outputFile << "My\n";
    outputFile << "Balls\n";

    // close the file
    outputFile.close();
    cout << "Done.\n";
    return 0;


}